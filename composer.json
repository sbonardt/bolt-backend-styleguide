{
    "name": "sbonardt/bolt-backend-styleguide",
    "description": "An extension to display the Bolt Backend Styleguide.",
    "type": "bolt-extension",
    "keywords": [
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bob den Otter",
            "email": "bob@twokings.nl"
        },
        {
            "name": "Sebastiaan Bonardt",
            "email": "sebastiaan@twokings.nl"
        }

    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Sbonardt\\BackendStyleguide\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Sbonardt\\BackendStyleguide\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Sbonardt\\BackendStyleguide\\BackendStyleguideExtension"
    }
}
